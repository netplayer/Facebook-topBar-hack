{
  "manifest_version": 2,

  "name": "Facebook top bar hack",
  "description": "This extension makes facebook top bar always visible",
  "version": "0.0.0.1",
   "content_scripts": [
    {
      "matches": ["https://www.facebook.com/*"],
      "js": ["jquery-1.11.0.min.js", "myscript.js"]
          }
  ],
  "browser_action": {
    "default_icon": "icon.png"
      },
  "permissions": [
    "activeTab",
    "https://www.facebook.com/*"
  ]
}